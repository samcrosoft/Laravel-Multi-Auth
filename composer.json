{
  "name": "samcrosoft/multi-auth",
  "description": "A laravel package that allows for use of multiple sessions in a laravel project, it allows for multiple simultaneous logins",
  "require": {
    "illuminate/session": "^5.0",
    "illuminate/support": "^5.0",
    "illuminate/auth": "^5.0"
  },
  "keywords": [
    "laravel",
    "authentication",
    "auth",
    "session",
    "multi session"
  ],
  "require-dev": {
    "symfony/var-dumper": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Samcrosoft\\MultiAuth\\": "src/MultiAuth"
    }
  },
